<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean id="orders" class="com.qujia.Spring.beanLife.Orders" init-method="init_Method" destroy-method="destroyMethod">
        <property name="oname" value="手机"></property>
    </bean>
    <!--配置后置处理器  为当前所有bean都添加bean后置处理-->
    <bean id="myBeanPost" class="com.qujia.Spring.beanLife.MyBeanPost"></bean>
</beans>